class GfG {
    public static void main(String args[]) {
        for (int i = 0; i < 2; i++) {
            for (int j = 0; j < 2; j++) {
                // terminate inner loop when j=1
                if (j == 1)
                    break;

                System.out.println(j);
            }
        }
    }
}
